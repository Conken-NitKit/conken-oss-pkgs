{
  "name": "@unwheel-pkg/tailwind-support",
  "version": "0.0.0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Conken-NitKit/typescript-everything/issues"
  },
  "files": [
    "/lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "source": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/typescript/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "rimraf ./lib && tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "lint": "eslint \"src/**/*.{js,ts,jsx,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts,jsx,tsx}\" --fix",
    "test": "jest --passWithNoTests --coverage",
    "test:ci": "jest --passWithNoTests --coverage",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@unwheel-pkg/eslint-config": "*",
    "@unwheel-pkg/tsconfig": "*",
    "jest": "^29.3.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.0.3",
    "tsc-alias": "^1.7.1",
    "typescript": "^4.9.3"
  }
}
