{
  "name": "@unwheel-pkg/tailwind-support",
  "version": "0.0.0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Conken-NitKit/typescript-everything/issues"
  },
  "files": [
    "/lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "source": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/typescript/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "rimraf ./lib && tsc --project tsconfig.build.json",
